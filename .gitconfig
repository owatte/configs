[user]
	name = Josh Bohde
	email = josh.bohde@gmail.com

[alias]
    st = status -s
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
    who = shortlog -s --
[push]
	default = tracking
[branch]
	autosetupmerge = true
	autosetuprebase = always
[core]
	excludesfile = /home/joshbohde/.git/.gitignore
